<Window x:Class="ColorPickerProject.SelectColorLineWindow0"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ColorPickerProject"
                xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        mc:Ignorable="d"
        Title="SelectColorLineWindow0" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
            <Label Content="Line color:"/>
            <xctk:ColorPicker Name="colorPicker11" SelectedColorChanged="colorPicker11_SelectedColorChanged" Height="100" ></xctk:ColorPicker>
            <Button Content="Select" Click="Button_Click11" Width="50" Height="30"/>
        </StackPanel>
    </Grid>
</Window>
